.row(ng-controller='testCtrl')
  .col-md-12
    button(ng-click='fspull()').btn.btn-sm.btn-success 推文件
  .col-md-12
    button(ng-click='resScreen(true)').btn.btn-sm.btn-warning 响应屏幕操作
    button(ng-click='resScreen(false)').btn.btn-sm.btn-danger 不响应屏幕操作
  .col-md-12
    .btn.btn-warning(ng-click='debugStart()') start debug
  .col-md-12
    .btn.btn-warning(ng-click='debugStop()') stop debug
  .col-md-12
    .btn.btn-warning(ng-click='getAppUIEnd()') getAppUIEnd
  .col-md-12
    .btn.btn-warning(ng-click='getAppUI()') getAppUI && start
    img(id='crop')
  .col-md-12
    .btn.btn-warning(ng-click='scriptDebug()') debug

